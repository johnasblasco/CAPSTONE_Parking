{
  "version": 2,
  "builds": [
    {
      "src": "routes/**/*.js",
      "use": "@vercel/node@1.0.0"
    }
  ],
  "routes": [
    {
      "src": "/admin(.*)",
      "dest": "/routes/adminR.js"
    },
    {
      "src": "/earnings(.*)",
      "dest": "/routes/earningsR.js"
    },
    {
      "src": "/user(.*)",
      "dest": "/routes/userR.js"
    },
    {
      "src": "/vehicle(.*)",
      "dest": "/routes/vehicleR.js"
    }
  ]
}
